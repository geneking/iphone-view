{
  "name": "iphone-view",
  "version": "0.1.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch": "webpack --watch",
    "mac": "node_modules/.bin/electron .",
    "win": "node_modules\\.bin\\electron .",
    "build": "webpack",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "build:mac": "webpack && electron-builder --mac",
    "build:win": "webpack && electron-builder --win --x64",
    "build:linux": "webpack && electron-builder --linux",
    "build:all": "webpack && electron-builder -mwl",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jinweigang",
  "license": "ISC",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-toastify": "^3.1.0",
    "webpack-uglify-parallel": "^0.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "open-browser-webpack-plugin": "0.0.5",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "build": {
    "appId": "com.king.iphoneview",
    "copyright": "King",
    "productName": "iPhoneView",
    "dmg": {
      "window": {
        "x": 100,
        "y": 100,
        "width": 500,
        "height": 300
      }
    },
    "mac": {
      "icon": "./app/assets/icon.icns"
    },
    "win": {
      "icon": "./app/assets/icon.ico"
    }
  }
}
